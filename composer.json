{
    "name": "xyu/hyperf-captcha",
    "description": "A captcha componet for hyperf .",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "captcha"
    ],
    "require": {
        "php": ">=7.2",
        "psr/container": "^1.0|^2.0",
        "hyperf/session": "~2.2.0"
    },
    "require-dev": {
        "hyperf/di": "~2.2.0",
        "hyperf/utils": "~2.2.0"
    },
    "autoload": {
        "psr-4": {
            "Xyu\\HyperfCaptcha\\": "src/"
        },
        "files": [
            "src/Support/helper.php"
        ]
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\Captcha\\ConfigProvider"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
